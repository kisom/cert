.TH "CERT" "1" "Nov 2025" "github.com/kisom/cert" ""

.SH NAME
cert-completion-fish - Generate the autocompletion script for fish


.SH SYNOPSIS
\fBcert completion fish [flags]\fP


.SH DESCRIPTION
Generate the autocompletion script for the fish shell.

.PP
To load completions in your current shell session:

.EX
cert completion fish | source
.EE

.PP
To load completions for every new session, execute once:

.EX
cert completion fish > ~/.config/fish/completions/cert.fish
.EE

.PP
You will need to start a new shell for this setup to take effect.


.SH OPTIONS
\fB-h\fP, \fB--help\fP[=false]
	help for fish

.PP
\fB--no-descriptions\fP[=false]
	disable completion descriptions


.SH OPTIONS INHERITED FROM PARENT COMMANDS
\fB--ca\fP=""
	CA certificate bundle file

.PP
\fB--config\fP=""
	config file (default is $HOME/.config/goutils/cert.yaml)

.PP
\fB--debug\fP[=false]
	enable debug mode

.PP
\fB-d\fP, \fB--display-mode\fP="lower"
	hex display mode for SKI

.PP
\fB-i\fP, \fB--intermediates-file\fP=""
	intermediate certificate bundle

.PP
\fB-q\fP, \fB--quiet\fP[=false]
	enable quiet mode

.PP
\fB-k\fP, \fB--skip-verify\fP[=false]
	skip certificate verification

.PP
\fB--strict-tls\fP[=false]
	use strict TLS settings

.PP
\fB-v\fP, \fB--verbose\fP[=false]
	verbose output


